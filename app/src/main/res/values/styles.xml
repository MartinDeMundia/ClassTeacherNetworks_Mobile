<resources>

    <style name="cellTextviewStyle" parent="android:Widget.TextView">
        <item name="android:textColor">#000</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorPrimary</item>

    </style>

    <style name="AppThemeRed" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorCommonRed</item>

    </style>

    <style name="viewDivider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">2dp</item>
        <item name="android:background">@color/colorGray</item>
    </style>

    <style name="viewDividerWhite">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/colorWhite</item>
    </style>

    <style name="viewDividerPrimary">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/colorPrimary</item>
    </style>

    <style name="relative_CardLook">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/card_look</item>
    </style>

    <style name="relative_Role">
        <item name="android:layout_width">@dimen/_75sdp</item>
        <item name="android:layout_height">@dimen/_75sdp</item>
        <item name="android:layout_margin">@dimen/_5sdp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:background">@drawable/login_user_role_back</item>
    </style>

    <style name="image_Role">
        <item name="android:layout_width">@dimen/_45sdp</item>
        <item name="android:layout_height">@dimen/_45sdp</item>
        <item name="android:layout_margin">@dimen/_5sdp</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="text_Role">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:padding">@dimen/_5sdp</item>
        <item name="android:textColor">@color/colorWhite</item>
    </style>


    <style name="edt_Login_EditText">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/_250sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="met_primaryColor">?colorPrimaryDark</item>
        <item name="met_floatingLabel">highlight</item>
        <item name="met_iconPadding">5dp</item>
        <item name="android:background">@color/colorWhite</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="btn_Button_2">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">@dimen/_250sdp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/_10sdp</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:background">@drawable/button_2</item>
        <item name="android:layout_marginTop">15dp</item>
    </style>


    <style name="linear_Button_1">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/_5sdp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">@dimen/_10sdp</item>
    </style>
    <style name="pay_btn">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">4dp</item>
    </style>

    <style name="linear_Dashboard_1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingLeft">@dimen/_10sdp</item>
        <item name="android:paddingRight">@dimen/_10sdp</item>
        <item name="android:paddingBottom">@dimen/_6sdp</item>
        <item name="android:paddingTop">@dimen/_6sdp</item>
        <item name="android:gravity">center</item>
        <item name="android:foreground">?android:attr/selectableItemBackground</item>
    </style>

    <style name="linear_Button_Text_1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">@color/colorWhite</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">@font/quicksand_bold</item>
        <item name="android:textSize">@dimen/_10sdp</item>
    </style>


    <style name="linear_Button_Text_2">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">@font/quicksand_medium</item>
        <item name="android:textSize">@dimen/_9sdp</item>
    </style>

    <style name="Theme.FullTransparent" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <style name="MineCustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">9sp</item>
    </style>

    <style name="MineCustomTabText1" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">10sp</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


</resources>
